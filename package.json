{
  "name": "kyblik",
  "version": "0.1.0",
  "description": "Small language which compiles into JavaScript",
  "main": "lib/index",
  "repository": "kocisov/kyblik",
  "author": "kocisov",
  "license": "MIT",
  "bin": {
    "kyblik": "bin/kyblik"
  },
  "scripts": {
    "compile": "yarn jison && yarn rollup && yarn copy-parser && yarn rm-parser",
    "copy-parser": "cp parser.js lib/parser.js",
    "jison": "jison src/parser.jison",
    "one": "jison src/parser.jison && rollup -c && cp parser.js lib/parser.js && rimraf parser.js",
    "prepublish": "yarn one",
    "prettier": "prettier --write src/**/*.js --single-quote",
    "rm-parser": "rimraf parser.js",
    "rollup": "rollup -c"
  },
  "devDependencies": {
    "jison": "^0.4.17",
    "prettier": "^1.3.1",
    "rimraf": "^2.6.1",
    "rollup": "^0.41.6"
  },
  "dependencies": {
    "chalk": "^1.1.3",
    "clear": "^0.0.1",
    "debounce": "^1.0.2",
    "fs-extra": "^3.0.1",
    "yargs": "^8.0.1"
  }
}
